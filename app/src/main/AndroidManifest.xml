<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.belajarandroid">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RatingBar"></activity>
        <activity android:name=".InsertDataGambar" />
        <activity android:name=".UploadGambar" />
        <activity
            android:name=".CrudActivity"
            android:label="@string/title_activity_crud"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TampilanSlide" />
        <activity android:name=".TampilanGrab" />
        <activity android:name=".MoveForResultActivity" />
        <activity android:name=".MoveWithObjectActivity" />
        <activity android:name=".MoveWithDataActivity" />
        <activity android:name=".MoveActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>